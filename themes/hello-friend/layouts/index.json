{{- $.Scratch.Add "index" slice -}}
{{- range .Site.RegularPages -}}
    {{- $.Scratch.Add "index" (dict "title" .Title "tags" .Params.tags "permalink" .Permalink "content" (.Plain | safeHTML) "date" (.Date.Format "2006.01.02") "month" (.Date.Month) "readTime" .ReadingTime "categories" .Params.categories ) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
