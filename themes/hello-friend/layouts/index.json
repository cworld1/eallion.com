{{- $.Scratch.Add "index" slice -}}
{{- range .Site.RegularPages -}}
    {{- $.Scratch.Add "index" (dict "title" .Title "tags" .Params.tags "permalink" .Permalink "summary" (.Summary | safeHTML) "date" (.Date.Format "2006 年 01 月 02 日") "month" (.Date.Month) "readTime" .ReadingTime "categories" .Params.categories ) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
