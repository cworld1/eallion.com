{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.content.pageFolders": [
    {
      "title": "content",
      "path": "d:/gpm/github.com/eallion/eallion.com/content/posts/daily"
    }
  ],
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.site.baseURL": "https://eallion.com",
  "frontMatter.taxonomy.categories": [
      "日志",
      "代码",
      "运营",
      "分享",
      "山贼"
  ],
  "frontMatter.preview.host": "http://localhost:1313",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "previewPath": null,
      "pageBundle": false,
      "fields": [
        {
          "title": "标题",
          "name": "title",
          "type": "string"
        },
        {
          "title": "摘要",
          "name": "description",
          "type": "string"
        },
        {
          "title": "发布日期",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "最后修改日期",
          "name": "lastmod",
          "type": "datetime"
        },
        {
          "title": "文章配图",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "草稿",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "标签",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "分类",
          "name": "categories",
          "type": "categories"
        }
      ]
    }
  ]
}
